{
  "expo": {
    "name": "Santo POS",
    "slug": "SantoPOS",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["android"],
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#ffc107",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffc107"
    },
    "android": {
      "package": "david.personal.santopos",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFC107"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#FFC107"
    }
  }
}

{
  "expo": {
    "name": "Las Santas Ventas",
    "slug": "LasSantasVentas",
    "privacy": "public",
    "platforms": [
      "android"
    ],
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#ffc107",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffc107"
    },
    "android": {
      "package": "david.personal.lassantasventas",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#FFC107"
    }
  }
}
